    "body-parser": "^1.18.3",
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "react-bootstrap": "^1.5.0"
  },
  "devDependencies": {
    "concurrently": "^6.1.0"
  }
